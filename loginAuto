#!/bin/zsh

source ~/git-repository/myBackup/shell/echoRed

echoRed 'BACKING UP CONFIGURATION...'

mackup backup
cd ~/git-repository/myBackup
git add --all
git commit -a -m "normal sync"
git push origin master

print ''
echoRed 'BACKING UP GIT-REPOSITORY...'
cd ~/git-repository/Note
git add --all
git commit -a -m 'normal sync'
git push origin master

print ''
echoRed 'CHECK BREW OUTDATED...'
brew outdated

print ''
echoRed '--------------'
echoRed 'Finished.'
